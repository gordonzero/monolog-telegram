{
    "name": "gordonzero/monolog-telegram",
    "description": "Laravel 5.6 Monolog custom telegram channel",
    "keywords": ["laravel", "laravel56", "log", "monolog", "telegram","bot"],
    "homepage": "https://github.com/gordonzero/monolog-telegram",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Tombrello",
            "email": "gordonzero@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "irazasyed/telegram-bot-sdk": "^3.0"
    },
    "autoload": {
        "psr-0": {
            "Logger": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Logger\\Laravel\\Provider\\MonologTelegramHandlerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}